@model List<string>
@{
    ViewData["Title"] = "DriveFileList";
}

<h1>DriveFileList</h1>
<hr />

<div class="container">
    <div class="row">
        <div class="col-lg-3"></div>
        <div class="col-lg-3">
            @foreach (var file in Model)
            {
                @:<p>@file<p/>
            }
        </div>
    </div>
</div>

